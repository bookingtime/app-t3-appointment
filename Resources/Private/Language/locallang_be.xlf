				<source>DELETE bookingtimepage URL: &lt;u&gt;%s&lt;/u&gt;</source>
			</trans-unit>
			<trans-unit id="flashmessage.delete.body">
				<source>You have succesfully deleted: &lt;u&gt;%s&lt;/u&gt;</source>
			</trans-unit>

			<trans-unit id="flashmessage.delete.title.error">
				<source>DELETE bookingtimepage URL failed!</source>
			</trans-unit>
