				<source>Your email address</source>
			</trans-unit>
			<trans-unit id="step2.form.email.placeholder">
				<source>e. g. example@company.com</source>
			</trans-unit>
			<trans-unit id="step2.form.firstname.label">
				<source>First name</source>
			</trans-unit>
			<trans-unit id="step2.form.firstname.placeholder">
				<source>John</source>
			</trans-unit>
			<trans-unit id="step2.form.lastname.label">
				<source>Last name</source>
			</trans-unit>
			<trans-unit id="step2.form.lastname.placeholder">
				<source>Doe</source>
			</trans-unit>
			<trans-unit id="step2.form.company.label">
				<source>Company</source>
			</trans-unit>
			<trans-unit id="step2.form.company.placeholder">
				<source>Name of your company</source>
			</trans-unit>
			<trans-unit id="step2.form.checkbox.terms">
